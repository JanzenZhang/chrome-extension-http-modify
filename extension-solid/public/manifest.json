{
  "manifest_version": 3,
  "name": "HTTP Header Modifier",
  "version": "1.0",
  "description": "Intercept and add custom HTTP headers to all requests.",
  "permissions": [
    "declarativeNetRequest",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.svg"
  },
  "icons": {
    "128": "icon.svg"
  },
  "background": {
    "service_worker": "background.js"
  }
}
